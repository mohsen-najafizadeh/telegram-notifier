      "MohsenNajafizadeh\\TelegramNotifier\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Peyman Manteali",
      "email": "peyman.manteali@gmail.com"
    },
    {
      "name": "Mohsen Najafizadeh",
      "email": "mohsennajafizadeh23@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "preferred-install": "dist",
